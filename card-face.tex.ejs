\section{<%- name %>}
<% if (desc) { -%>
{\cardbodyfont
<% for (let line of desc.split('\n')) { -%>
<%- line.trim() %>\par
<% } -%>
}
<% } -%>
<% if (prompts.length > 0) { -%>
{\cardpromptfont
\begin{itemize}[leftmargin=0.3cm]
\tightlist
<% for (let prompt of prompts) { -%>
\item[-] <%- prompt %>
<% } -%>
\end{itemize}
}
<% } -%>
\vspace*{\fill}
<% if (rule) { -%>
\begin{adjustwidth}{1.5cm}{1.5cm}
{\cardrulefont\center\emph
<%- rule %>\par
}
\end{adjustwidth}
<% } -%>
\begin{tikzpicture}[remember picture,overlay,shift={(current page.south west)}]
<% for (let i = 0; i < icons.length; i++) { -%>
\node[anchor=south west, xshift=-0.1cm, yshift=<%- (i - 0.1) %>cm]{
    \includegraphics[width=1cm]{static/nandeck/<%- icons[i] %>}
};
<% } -%>
\end{tikzpicture}
<% if (img) { -%>
\begin{tikzpicture}[remember picture, overlay]
\node[inner sep=0pt, opacity=0.2, xshift=-0.42cm] at (current page.center) {%
\includegraphics[width=3in,height=3in]{static/nandeck/<%- img %>}%
};%
\end{tikzpicture}
<% } -%>